          "_customHTML": "<!-- Type valid HTML here -->\n<section id=\"settings\" class=\"settings dark mbr-section\">\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"mbr-text\">\n                <p class=\"mbr-fonts-style block-text\"><a href=\"#\" onclick=\"javascript:toggleSettings('inuser')\"><span class=\"mbri-user mbr-iconfont mbr-iconfont-btn\"></span></a></p>\n            </div>\n        </div>\n    </div>\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"mbr-text\">\n                <p class=\"mbr-fonts-style block-text\"><a href=\"#\" onclick=\"javascript:toggleSettings('insettings')\"><span class=\"mbri-setting3 mbr-iconfont mbr-iconfont-btn\"></span></a></p>\n            </div>\n        </div>\n    </div>\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"mbr-text\">\n                <p class=\"mbr-fonts-style block-text\"><a href=\"#\" onclick=\"javascript:toggleSettings('inpreferences')\"><span class=\"mbri-setting2 mbr-iconfont mbr-iconfont-btn\"></span></a></p>\n            </div>\n        </div>\n    </div>\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"mbr-text\">\n                <p class=\"mbr-fonts-style block-text\"><a href=\"#\" onclick=\"javascript:toggleSettings('instyle')\"><span class=\"mbri-change-style mbr-iconfont mbr-iconfont-btn\"></span></a></p>\n            </div>\n        </div>\n    </div>\n<hr>\n    <div class=\"container\">\n        <div class=\"media-container-row\">\n            <div class=\"mbr-text\">\n                <p class=\"mbr-fonts-style block-text\"><a href=\"#\" onclick=\"alert('options saved')\"><span class=\"mbr-iconfont icon54-v4-save mbri-icon\"></span></a></p>\n            </div>\n        </div>\n    </div>\n</section>\n",
